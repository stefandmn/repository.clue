<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repository.clue" name="Clue Repository" version="2.0.1" provider-name="AMSD">
	<requires>
		<import addon="xbmc.addon" version="12.0.0"/>
	</requires>
	<extension point="xbmc.addon.repository">
		<info>https://amsd.go.ro/clue/repos/addons/addons.xml</info>
		<checksum verify="sha256">https://amsd.go.ro/clue/repos/addons/addons.xml.sha256</checksum>
		<datadir zip="true">https://amsd.go.ro/clue/repos/addons</datadir>
		<hashes>sha256</hashes>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Clue Repository for Kodi</summary>
		<description>The Clue Repository add-on provides the main reference for Kodi programs, services, skins, and many other addons within Clue system or for a particular Kodi instance.</description>
		<platform>all</platform>
	</extension>
</addon>
